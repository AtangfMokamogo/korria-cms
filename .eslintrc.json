{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-classes-per-file": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-shadow": "off",
        "no-restricted-syntax": [
          "error",
          "LabeledStatement",
          "WithStatement"
        ]
    }
}
